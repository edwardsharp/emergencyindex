  <%= search_form_for @q, url: '/admin/projects-list', remote: true, class: 'row' do |f| %>
	  <div class="input-field-fix col s3 l2">

		  <%= f.check_box :published_true %>
		  <%= f.label :published_true, 'Published' %>

		</div>

		<div class="col s3 l4">
			
    	<%= f.select :volume_year_in, Volume.all.collect {|p| [ p.year, p.year ] }.unshift(['Volume','Volume']), { disabled: 'Volume'}, multiple: true %>
		</div>
		<div class="input-field col s4 l5">
		  <%= f.search_field :name_cont %>
		  <%= f.label :name_cont, "Search" %>
		</div>
		<div class="input-field col s2 l1">
			<button class="btn waves-effect waves-light" type="submit" name="submit">
		    <i class="material-icons">search</i>
		  </button>
		</div>
	<% end %>

